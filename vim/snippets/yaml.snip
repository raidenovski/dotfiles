# K8S Deployment
snippet Deployment
alias deploy
  ---
  apiVersion: apps/v1beta2
  kind: Deployment
  metadata:
    name: ${1:name}
    namespace: ${2:default}
    labels:
      app: ${3:$1}
  spec:
    replicas: ${4:1}
    selector:
      matchLabels:
        app: $3
    template:
      metadata:
        labels:
          app: $3
      spec:
        containers:
          - name: ${4:$3}
            image: ${5:image}

